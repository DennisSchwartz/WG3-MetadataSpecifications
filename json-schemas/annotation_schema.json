{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "DATS annotation schema",
  "description": "A pair of value (string or numeric) with a corresponding ontology term (IRI), if applicable.",
  "type": "object",
  "properties": {
    "value": {
      "oneOf": [
        { "type": "string" },
        { "type": "number"}
      ]
    },
    "ontologyTermIRI": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": false
}